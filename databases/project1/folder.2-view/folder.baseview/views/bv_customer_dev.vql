# REQUIRES-PROPERTIES-FILE - # Do not remove this comment!
CREATE OR REPLACE WRAPPER JDBC bv_customer_dev
    FOLDER = '/2-view/baseview'
    DATASOURCENAME=denodo_training.ds_sql_server_training
    CATALOGNAME='${databases.project1.folder.2-view.folder.baseview.views.jdbc.bv_customer_dev.CATALOGNAME}' ESCAPE
    SCHEMANAME='${databases.project1.folder.2-view.folder.baseview.views.jdbc.bv_customer_dev.SCHEMANAME}' 
    RELATIONNAME='Customer' 
    OUTPUTSCHEMA (
        customerid = 'CustomerID' :'java.lang.Integer' (OPT) (sourcetypedecimals='0', sourcetyperadix='10', sourcetypesize='10', sourcetypeid='4', sourcetypename='int identity')  NOT NULL SORTABLE IS_AUTOINCREMENT,
        namestyle = 'NameStyle' :'java.lang.Boolean' (OPT) (sourcetypesize='1', sourcetypeid='-7', sourcetypename='NameStyle')  NOT NULL NOT SORTABLE,
        title = 'Title' :'java.lang.String' (OPT) (sourcetypesize='8', sourcetypeid='-9', sourcetypename='nvarchar')  SORTABLE,
        firstname = 'FirstName' :'java.lang.String' (OPT) (sourcetypesize='50', sourcetypeid='-9', sourcetypename='Name')  NOT NULL NOT SORTABLE,
        middlename = 'MiddleName' :'java.lang.String' (OPT) (sourcetypesize='50', sourcetypeid='-9', sourcetypename='Name')  NOT SORTABLE,
        lastname = 'LastName' :'java.lang.String' (OPT) (sourcetypesize='50', sourcetypeid='-9', sourcetypename='Name')  NOT NULL NOT SORTABLE,
        suffix = 'Suffix' :'java.lang.String' (OPT) (sourcetypesize='10', sourcetypeid='-9', sourcetypename='nvarchar')  SORTABLE,
        companyname = 'CompanyName' :'java.lang.String' (OPT) (sourcetypesize='128', sourcetypeid='-9', sourcetypename='nvarchar')  SORTABLE,
        salesperson = 'SalesPerson' :'java.lang.String' (OPT) (sourcetypesize='256', sourcetypeid='-9', sourcetypename='nvarchar')  SORTABLE,
        emailaddress = 'EmailAddress' :'java.lang.String' (OPT) (sourcetypesize='50', sourcetypeid='-9', sourcetypename='nvarchar')  SORTABLE,
        phone = 'Phone' :'java.lang.String' (OPT) (sourcetypesize='25', sourcetypeid='-9', sourcetypename='Phone')  NOT SORTABLE,
        passwordhash = 'PasswordHash' :'java.lang.String' (OPT) (sourcetypesize='128', sourcetypeid='12', sourcetypename='varchar')  NOT NULL SORTABLE,
        passwordsalt = 'PasswordSalt' :'java.lang.String' (OPT) (sourcetypesize='10', sourcetypeid='12', sourcetypename='varchar')  NOT NULL SORTABLE,
        rowguid = 'rowguid' :'java.lang.String' (OPT) (sourcetypesize='36', sourcetypeid='1', sourcetypename='uniqueidentifier')  NOT NULL NOT SORTABLE,
        modifieddate = 'ModifiedDate' :'java.time.LocalDateTime' (OPT) (sourcetypedecimals='3', sourcetypesize='23', sourcetypeid='93', sourcetypename='datetime')  NOT NULL SORTABLE
    )
    CONSTRAINT 'PK_Customer_CustomerID' PRIMARY KEY ( 'CustomerID' )
    INDEX 'PK_Customer_CustomerID' CLUSTER UNIQUE PRIMARY ( 'CustomerID' )
    INDEX 'AK_Customer_rowguid' OTHER UNIQUE ( 'rowguid' )
    INDEX 'IX_Customer_EmailAddress' OTHER ( 'EmailAddress' );

CREATE OR REPLACE TABLE bv_customer_dev I18N us_pst (
        customerid:int (notnull, autoincrement, sourcetypeid = '4', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '10'),
        namestyle:boolean (notnull, sourcetypeid = '-7', sourcetypesize = '1'),
        title:text (sourcetypeid = '-9', sourcetypesize = '8'),
        firstname:text (notnull, sourcetypeid = '-9', sourcetypesize = '50'),
        middlename:text (sourcetypeid = '-9', sourcetypesize = '50'),
        lastname:text (notnull, sourcetypeid = '-9', sourcetypesize = '50'),
        suffix:text (sourcetypeid = '-9', sourcetypesize = '10'),
        companyname:text (sourcetypeid = '-9', sourcetypesize = '128'),
        salesperson:text (sourcetypeid = '-9', sourcetypesize = '256'),
        emailaddress:text (sourcetypeid = '-9', sourcetypesize = '50'),
        phone:text (sourcetypeid = '-9', sourcetypesize = '25'),
        passwordhash:text (notnull, sourcetypeid = '12', sourcetypesize = '128'),
        passwordsalt:text (notnull, sourcetypeid = '12', sourcetypesize = '10'),
        rowguid:text (notnull, sourcetypeid = '1', sourcetypesize = '36'),
        modifieddate:timestamp (notnull, sourcetypeid = '93', sourcetypedecimals = '3', sourcetypesize = '23')
    )
    FOLDER = '/2-view/baseview'
    CONSTRAINT 'PK_Customer_CustomerID' PRIMARY KEY ( 'customerid' )
    CACHE OFF
    TIMETOLIVEINCACHE DEFAULT
    ADD SEARCHMETHOD bv_customer(
        I18N us_pst
        CONSTRAINTS (
             ADD customerid (any) OPT ANY
             ADD namestyle (any) OPT ANY
             ADD title (any) OPT ANY
             ADD firstname (any) OPT ANY
             ADD middlename (any) OPT ANY
             ADD lastname (any) OPT ANY
             ADD suffix (any) OPT ANY
             ADD companyname (any) OPT ANY
             ADD salesperson (any) OPT ANY
             ADD emailaddress (any) OPT ANY
             ADD phone (any) OPT ANY
             ADD passwordhash (any) OPT ANY
             ADD passwordsalt (any) OPT ANY
             ADD rowguid (any) OPT ANY
             ADD modifieddate (any) OPT ANY
        )
        OUTPUTLIST (companyname, customerid, emailaddress, firstname, lastname, middlename, modifieddate, namestyle, passwordhash, passwordsalt, phone, rowguid, salesperson, suffix, title
        )
        WRAPPER (jdbc bv_customer_dev)
    );

